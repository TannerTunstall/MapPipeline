<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>Sample Locations</name>
    <description>Sample KML file for testing MapPipeline</description>

    <Style id="defaultStyle">
      <IconStyle>
        <scale>1.0</scale>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/paddle/red-circle.png</href>
        </Icon>
      </IconStyle>
    </Style>

    <Placemark>
      <name>New York City</name>
      <description>The Big Apple - largest city in the United States</description>
      <styleUrl>#defaultStyle</styleUrl>
      <Point>
        <coordinates>-74.006,40.7128,0</coordinates>
      </Point>
    </Placemark>

    <Placemark>
      <name>Los Angeles</name>
      <description>City of Angels - entertainment capital of the world</description>
      <styleUrl>#defaultStyle</styleUrl>
      <Point>
        <coordinates>-118.2437,34.0522,0</coordinates>
      </Point>
    </Placemark>

    <Placemark>
      <name>Chicago</name>
      <description>The Windy City - known for architecture and deep-dish pizza</description>
      <styleUrl>#defaultStyle</styleUrl>
      <Point>
        <coordinates>-87.6298,41.8781,0</coordinates>
      </Point>
    </Placemark>

    <Placemark>
      <name>San Francisco</name>
      <description>The Golden Gate City - tech hub of the world</description>
      <styleUrl>#defaultStyle</styleUrl>
      <Point>
        <coordinates>-122.4194,37.7749,0</coordinates>
      </Point>
    </Placemark>

    <Placemark>
      <name>Seattle</name>
      <description>The Emerald City - home of coffee culture</description>
      <styleUrl>#defaultStyle</styleUrl>
      <Point>
        <coordinates>-122.3321,47.6062,0</coordinates>
      </Point>
    </Placemark>
  </Document>
</kml>
